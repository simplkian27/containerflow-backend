    "version": "0.2.0",
    "configurations": [
      {
        "type": "java",
        "name": "Java: Launch current file",
        "request": "launch",
        "mainClass": "${file}",
        "cwd": "${fileDirname}",
        "console": "integratedTerminal"
      },
      {
        "type": "java",
        "name": "Java: Launch main class",
        "request": "launch",
        "mainClass": "",
        "projectName": "",
        "cwd": "${workspaceFolder}",
        "console": "integratedTerminal"
      },
      {
        "type": "java",
        "name": "Java: Attach to running app",
        "request": "attach",
        "hostName": "localhost",
        "port": 5005,
        "timeout": 30000
      }
    ]
}
